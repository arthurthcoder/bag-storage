/* @bag:start(grid) */
/* ########################## START GRID ########################## */
.grid.flex {
    -moz-flex-wrap: wrap;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}

.grid:not(.flex)::after {
    content: '';
    display: block;
    clear: both;
}

.grid:not(.flex) > * {
    float: left;
}

.grid[class*="col-"] > * {
    width: 100%;
    max-width: -moz-calc((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * (var(--grid-col, 1) - 1)) / var(--grid-col, 1));
    max-width: -webkit-calc((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * (var(--grid-col, 1) - 1)) / var(--grid-col, 1));
    max-width: calc((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * (var(--grid-col, 1) - 1)) / var(--grid-col, 1));
    margin-right: var(--grid-gap-x, var(--grid-gap, 0));
    margin-bottom: var(--grid-gap-y, var(--grid-gap, 0));
}

.grid [class*="col-"] {
    width: 100%;
    max-width: -moz-calc(((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * 11) / 12 + var(--grid-gap-x, var(--grid-gap, 0px))) * var(--grid-col, 1) - var(--grid-gap-x, var(--grid-gap, 0px)));
    max-width: -webkit-calc(((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * 11) / 12 + var(--grid-gap-x, var(--grid-gap, 0px))) * var(--grid-col, 1) - var(--grid-gap-x, var(--grid-gap, 0px)));
    max-width: calc(((100% - var(--grid-gap-x, var(--grid-gap, 0px)) * 11) / 12 + var(--grid-gap-x, var(--grid-gap, 0px))) * var(--grid-col, 1) - var(--grid-gap-x, var(--grid-gap, 0px)));
    margin-right: var(--grid-gap-x, var(--grid-gap, 0));
    margin-bottom: var(--grid-gap-y, var(--grid-gap, 0));
}

.grid [class*="col-"]:last-of-type {
    margin-right: 0;
}

.grid.col-2 > * {--grid-col: 2;}
.grid.col-2 > *:nth-child(2n+0) {margin-right: 0;}

.grid.col-3 > * {--grid-col: 3;}
.grid.col-3 > *:nth-child(3n+0) {margin-right: 0;}

.grid.col-4 > * {--grid-col: 4;}
.grid.col-4 > *:nth-child(4n+0) {margin-right: 0;}

.grid.col-5 > * {--grid-col: 5;}
