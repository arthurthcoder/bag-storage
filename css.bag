.grid.col-9 > * {--grid-col: 9;}
.grid.col-9 > *:nth-child(9n+0) {margin-right: 0;}

.grid.col-10 > * {--grid-col: 10;}
.grid.col-10 > *:nth-child(10n+0) {margin-right: 0;}

.grid.col-11 > * {--grid-col: 11;}
.grid.col-11 > *:nth-child(11n+0) {margin-right: 0;}

.grid.col-12 > * {--grid-col: 12;}
.grid.col-12 > *:nth-child(12n+0) {margin-right: 0;}

.grid.col-full > * {max-width: 100%; margin-right: 0;}

.grid .col-2 {--grid-col: 2;}
.grid .col-3 {--grid-col: 3;}
.grid .col-4 {--grid-col: 4;}
.grid .col-5 {--grid-col: 5;}
.grid .col-6 {--grid-col: 6;}
.grid .col-7 {--grid-col: 7;}
.grid .col-8 {--grid-col: 8;}
.grid .col-9 {--grid-col: 9;}
.grid .col-10 {--grid-col: 10;}
.grid .col-11 {--grid-col: 11;}
.grid .col-full {max-width: 100%; margin-right: 0;}

